{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "https://homer-backend-nmnq.onrender.com/api/$1" },
    { "src": "/(.*)", "dest": "/frontend/dist/index.html" }
  ]
}
