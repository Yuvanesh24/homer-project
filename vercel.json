{
  "builds": [
    {
      "src": "frontend",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "cd frontend && npm install && npm run build",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "https://homer-backend-nmnq.onrender.com/api/$1" }
  ]
}
